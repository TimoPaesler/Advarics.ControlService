                </Style>

                <Style x:Key="InversCheckBoxStyle" TargetType="CheckBox">
                    <Setter Property="Foreground" Value="WhiteSmoke"/>
                    <Setter Property="Height" Value="22"/>
                </Style>

                <Style x:Key="MainRadioButtonStyle" TargetType="RadioButton">
                    <Setter Property="Height" Value="22"/>
                </Style>

                <Style x:Key="InversRadioButtonStyle" TargetType="RadioButton">
                    <Setter Property="Foreground" Value="WhiteSmoke"/>
                    <Setter Property="Height" Value="22"/>
                </Style>

                <Style x:Key="RadOutlookBarItemStyle" TargetType="rad:RadOutlookBarItem">
                    <!--<Setter Property="Foreground" Value="Black"/>
			<Setter Property="FontSize" Value="11"/>
			<Setter Property="FontFamily" Value="Verdana"/>-->
                    <Setter Property="FontWeight" Value="Bold"/>
                    <Setter Property="FontSize" Value="11"/>
                </Style>

                <Style x:Key="RadOutlookBarStyle" TargetType="rad:RadOutlookBar">
                    <!--<Setter Property="Foreground" Value="Black"/>
			<Setter Property="FontSize" Value="11"/>
			<Setter Property="FontFamily" Value="Verdana"/>-->
                    <Setter Property="FontWeight" Value="Bold"/>
                </Style>

                <Style TargetType="rad:RadTabControl" x:Key="TabControlTextStyle">
                    <!--<Setter Property="Foreground" Value="WhiteSmoke"/>
			<Setter Property="FontSize" Value="11"/>
			<Setter Property="FontFamily" Value="Verdana"/>
			<Setter Property="FontWeight" Value="Bold"/>-->
                </Style>

                <Style TargetType="rad:RadComboBox" x:Key="RadComboBoxStyle">
                    <!--<Setter Property="Foreground" Value="Black"/>
			<Setter Property="FontSize" Value="11"/>
			<Setter Property="FontFamily" Value="Verdana"/>
			<Setter Property="FontWeight" Value="Bold"/>-->
                    <Setter Property="IsEditable" Value="True"/>
                    <Setter Property="IsReadOnly" Value="False"/>
                    <!--<Setter Property="OpenDropDownOnFocus" Value="True"/>-->
                    <Setter Property="IsFilteringEnabled" Value="True"/>

                    <Setter Property="TextSearchMode" Value="StartsWith"/>
                    <Setter Property="CanAutocompleteSelectItems" Value="False"/>
                    <Setter Property="ClearSelectionButtonVisibility" Value="Visible"/>
