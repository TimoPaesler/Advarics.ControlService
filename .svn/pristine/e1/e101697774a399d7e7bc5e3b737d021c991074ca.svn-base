                    <!--<Setter Property="FontFamily" Value="Trebuchet MS"/>-->
                </Style>

        

                <Style x:Key="SizesHeadTextBlockStyle" TargetType="TextBlock">
                    <Setter Property="Foreground" Value="{StaticResource InverseTextBrush}"/>
                    <Setter Property="FontSize" Value="12"/>
                    <Setter Property="FontFamily" Value="Trebuchet MS"/>
                    <Setter Property="FontWeight" Value="Bold"/>
                    <Setter Property="TextAlignment" Value="Center"/>
                    <Setter Property="VerticalAlignment" Value="Center"/>
                    <Setter Property="Height" Value="22"/>
                </Style>

                <Style x:Key="SizeHeadLineStyle" TargetType="Line">
                    <Setter Property="Stroke" Value="Black"/>
                    <Setter Property="VerticalAlignment" Value="Center"/>
                </Style>




                <Style x:Key="MainTextBoxStyle" TargetType="TextBox">
                    <Setter Property="Height" Value="22"/>
                    <Setter Property="BorderThickness" Value="1"/>
                    <Setter Property="Background" Value="#FFFFFFFF"/>
                    <Setter Property="Foreground" Value="#FF000000"/>
                    <Setter Property="Padding" Value="2"/>
                    <Setter Property="BorderBrush">
                        <Setter.Value>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFA3AEB9" Offset="0"/>
                                <GradientStop Color="#FF8399A9" Offset="0.375"/>
                                <GradientStop Color="#FF718597" Offset="0.375"/>
                                <GradientStop Color="#FF617584" Offset="1"/>
                            </LinearGradientBrush>
                        </Setter.Value>
                    </Setter>
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="TextBox">
                                <Grid x:Name="RootElement">
                                    <Border x:Name="Border" Opacity="1" BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="3" BorderBrush="#4C00FFF1" Background="WhiteSmoke">
                                        <ScrollViewer Height="Auto" x:Name="ContentElement" Width="Auto" BorderThickness="0" IsTabStop="False" Padding="{TemplateBinding Padding}" VerticalAlignment="Top" Margin="1"/>
                                    </Border>
                                </Grid>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>




                <Style x:Key="MainMaskedTextBoxStyle" TargetType="rad:RadMaskedTextBox">
                    <Setter Property="Height" Value="22"/>
                </Style>



                <Style x:Key="MainMaskedNumericInputStyle" TargetType="rad:RadMaskedNumericInput">
                    <Setter Property="Height" Value="22"/>
                    <Setter Property="AutoFillNumberGroupSeparators" Value="False"/>
                    <Setter Property="SelectionOnFocus" Value="SelectAll"/>
                    <Setter Property="UpdateValueEvent" Value="LostFocus"/>
                </Style>



                <Style x:Key="MainMaskedCurrencyInputStyle" TargetType="rad:RadMaskedCurrencyInput">
                    <Setter Property="Height" Value="22"/>
                    <Setter Property="IsCurrencySymbolVisible" Value="False"/>
                    <!--Darf nicht angezeigt werden, weil Bug im Control: Cursor springt bei "Fr." zum Punkt vom Währungssymbol-->
                    <Setter Property="AutoFillNumberGroupSeparators" Value="False"/>
                    <Setter Property="SelectionOnFocus" Value="SelectAll"/>
                    <Setter Property="UpdateValueEvent" Value="LostFocus"/>
                </Style>


                <Style x:Key="MainPasswordBoxStyle" TargetType="PasswordBox">
                    <Setter Property="Height" Value="22"/>
                    <Setter Property="BorderThickness" Value="1"/>
                    <Setter Property="Background" Value="#FFFFFFFF"/>
                    <Setter Property="Foreground" Value="#FF000000"/>
                    <Setter Property="Padding" Value="2"/>
                    <Setter Property="BorderBrush">
                        <Setter.Value>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFA3AEB9" Offset="0"/>
                                <GradientStop Color="#FF8399A9" Offset="0.375"/>
                                <GradientStop Color="#FF718597" Offset="0.375"/>
                                <GradientStop Color="#FF617584" Offset="1"/>
                            </LinearGradientBrush>
                        </Setter.Value>
                    </Setter>
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="PasswordBox">
                                <Grid x:Name="RootElement">
                                    <Border x:Name="Border" Opacity="1" BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="3" BorderBrush="#4C00FFF1" Background="WhiteSmoke">
                                        <ScrollViewer Height="Auto" x:Name="ContentElement" VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Hidden" Width="Auto" BorderThickness="0" IsTabStop="False" Padding="{TemplateBinding Padding}" VerticalAlignment="Top" Margin="1"/>
                                    </Border>
                                </Grid>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>

                <Style x:Key="MainComboBoxStyle" TargetType="ComboBox">
