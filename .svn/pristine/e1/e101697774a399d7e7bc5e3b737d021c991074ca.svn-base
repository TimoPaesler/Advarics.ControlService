                <Style x:Key="HeaderTextBlockStyle" TargetType="TextBlock">
                    <Setter Property="Foreground" Value="Gray"/>
                    <Setter Property="FontSize" Value="14"/>
                    <Setter Property="FontFamily" Value="Trebuchet MS"/>
                    <Setter Property="FontWeight" Value="Bold"/>
                    <Setter Property="Height" Value="24"/>
                    <Setter Property="VerticalAlignment" Value="Center"/>
                </Style>


                <Style x:Key="InversTextBlockStyle" TargetType="TextBlock">
                    <Setter Property="Foreground" Value="{StaticResource InverseTextBrush}"/>
                    <Setter Property="FontWeight" Value="Bold"/>
                    <Setter Property="Height" Value="22"/>
                </Style>

                <Style x:Key="InversTextBlockStyleSmall" TargetType="TextBlock">
                    <Setter Property="Foreground" Value="{StaticResource InverseTextBrush}"/>
                    <Setter Property="Height" Value="22"/>
                </Style>

                <Style x:Key="InversTextBlockStyleBig" TargetType="TextBlock">
                    <Setter Property="Foreground" Value="{StaticResource InverseTextBrush}"/>
                    <Setter Property="Height" Value="22"/>
